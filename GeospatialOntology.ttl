         skos:definition "The nature or genre of the resource."@en-us .


###  http://schema.org/containedInPlace
schema:containedInPlace rdf:type owl:ObjectProperty ;
                        owl:inverseOf schema:containsPlace ;
                        rdfs:comment "The basic containment relation between a place and one that contains it."@en ;
                        rdfs:isDefinedBy "http://schema.org/" ;
                        rdfs:label "containedInPlace"@en .


###  http://schema.org/containsPlace
schema:containsPlace rdf:type owl:ObjectProperty ;
                     rdfs:comment "The basic containment relation between a place and another that it contains."@en ;
                     rdfs:isDefinedBy "http://schema.org/" ;
                     rdfs:label "containsPlace"@en .


###  http://ulusalharita.tr/ont/spo#hasRelation
spo:hasRelation rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain geo:SpatialObject ;
                rdfs:range geo:SpatialObject ;
                rdfs:label "Has relation"@en .


###  http://ulusalharita.tr/ont/spo#hasScale
spo:hasScale rdf:type owl:ObjectProperty ;
             rdfs:range spo:Scale ;
             rdfs:comment "Scale of geometry object"@en .


###  http://ulusalharita.tr/ont/spo#multipleRelation
spo:multipleRelation rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf spo:hasRelation ;
                     rdf:type owl:SymmetricProperty ;
                     rdfs:domain geo:SpatialObject ;
                     rdfs:range geo:SpatialObject ;
                     rdfs:comment "n:m relations between features"@en ;
                     rdfs:label "Multiple relation"@en .


###  http://ulusalharita.tr/ont/spo#sameAsRelation
spo:sameAsRelation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf spo:hasRelation ;
                   rdf:type owl:SymmetricProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain geo:SpatialObject ;
                   rdfs:range geo:SpatialObject ;
                   rdfs:comment "1:1 realation between features"@en ;
                   rdfs:label "Same as relation"@en .


###  http://ulusalharita.tr/ont/spo#subSetOf
spo:subSetOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf spo:hasRelation ;
             owl:inverseOf spo:superSetOf ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain geo:SpatialObject ;
             rdfs:range geo:SpatialObject ;
             rdfs:comment "n:1 relation between features"@en ;
             rdfs:label "Sub set of"@en .


###  http://ulusalharita.tr/ont/spo#superSetOf
spo:superSetOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf spo:hasRelation ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain geo:SpatialObject ;
               rdfs:range geo:SpatialObject ;
               rdfs:comment "1:n relation between features"@en ;
               rdfs:label "Super set of"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart
dul:hasPart rdf:type owl:ObjectProperty ;
            owl:inverseOf dul:isPartOf ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain geo:SpatialObject ;
            rdfs:range geo:SpatialObject ;
            rdfs:comment """A schematic relation between any entities, e.g. 'the human body has a brain as part', '20th century contains year 1923', 'World War II includes the Pearl Harbour event'.
Subproperties and restrictions can be used to specialize hasPart for objects, events, etc."""^^xsd:string ;
            rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
            rdfs:label "ha parte"@it ,
                       "has part"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf
dul:isPartOf rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain geo:SpatialObject ;
             rdfs:range geo:SpatialObject ;
             rdfs:comment "A relation between any entities, e.g.'brain is a part of the human body'."^^xsd:string ;
             rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
             rdfs:label "is part of"@en ,
                        "è parte di"@it .


###  http://www.opengis.net/ont/geosparql#defaultGeometry
geo:defaultGeometry rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf geo:hasGeometry ;
                    rdfs:domain geo:Feature ;
                    rdfs:range geo:Geometry ;
                    rdfs:comment """
      The default geometry to be used in spatial calculations.
      It is Usually the most detailed geometry.
    """@en ;
                    rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ,
                                     <http://www.opengis.net/spec/geosparql/1.0> ;
                    rdfs:label "defaultGeometry"@en ;
                    skos:prefLabel "defaultGeometry"@en .


###  http://www.opengis.net/ont/geosparql#hasGeometry
geo:hasGeometry rdf:type owl:ObjectProperty ;
                rdfs:domain geo:Feature ;
                rdfs:range geo:Geometry ;
                rdfs:comment """
      A spatial representation for a given feature.
    """@en ;
                rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ,
                                 <http://www.opengis.net/spec/geosparql/1.0> ;
                rdfs:label "hasGeometry"@en ;
                skos:prefLabel "hasGeometry"@en .


###  http://www.opengis.net/ont/geosparql#sfContains
geo:sfContains rdf:type owl:ObjectProperty ;
               rdfs:domain geo:SpatialObject ;
               rdfs:range geo:SpatialObject ;
